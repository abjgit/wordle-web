{
  "name": "wordle-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && tsc && tsc -p api/tsconfig.json && vite build",
    "clean": "rimraf .vercel/output && rimraf dist && rimraf api/dist",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "setup-bot": "node scripts/setup-bot.js",
    "build:scripts": "tsc -p scripts/tsconfig.json",
    "set-webhook": "npm run build:scripts && node --experimental-specifier-resolution=node scripts/dist/set-webhook.js"
  },
  "dependencies": {
    "@thirdweb-dev/react": "^4.9.4",
    "@thirdweb-dev/sdk": "^4.0.17",
    "buffer": "^6.0.3",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.17.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vercel/node": "^3.2.26",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "firebase-admin": "^13.0.1",
    "postcss": "^8.4.32",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "util": "^0.12.5",
    "vite": "^5.0.8"
  }
}
